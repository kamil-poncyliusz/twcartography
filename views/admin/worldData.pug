doctype html
html(lang="en")
  head
    meta(charset="UTF-8")
    meta(http-equiv="X-UA-Compatible", content="IE=edge")
    meta(name="viewport", content="width=device-width, initial-scale=1.0")
    title Admin Panel
    link(rel="stylesheet", href="/css/reset.css", type="text/css")
    link(rel="stylesheet", href="/css/main.css", type="text/css")
    link(rel="stylesheet", href="/css/admin/worldData.css", type="text/css")
    script(src="/scripts/worldData.js", type="module", defer)
  body
    table#world-data
      tbody
        each world in worlds
          tr
            td= world.server + world.num
            each worldData in world.world_data
              td= worldData.turn
